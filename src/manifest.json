{
  "manifest_version": 2,
  "name": "ChroMeme",
  "version": "1.0.0",
  "description": "An easy way to create memes using the MemeGen.link API",
  "browser_action": {
    "default_icon": "images/icon128.png",
    "default_title": "MemeGen"
  },
  "icons": {
    "32": "images/icon32.png",
    "64": "images/icon64.png",
    "128": "images/icon128.png" },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}